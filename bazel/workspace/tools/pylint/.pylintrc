[MASTER]
extension-pkg-whitelist=pydantic

[BASIC]
docstring-min-length=10

[MESSAGES CONTROL]
disable=missing-module-docstring,redefined-builtin,missing-class-docstring,invalid-name,too-many-instance-attributes,too-few-public-methods,fixme,too-many-arguments,too-many-public-methods,too-many-branches,too-many-locals
